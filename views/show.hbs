{{> header }}
<div class="container-fluid">
  <div class="row">
    <div class="col-md-6 col-md-offset-4" id="show-heading">
    </div>
  </div>
  <div class="row" id="storyTableContainer">
    <div class="col-md-9">
      
      <div class="row">
        <div class="col-md-12" id="missChart">
        </div>
      </div>

      <div class="row">
        <div class="col-md-12" id="speedChart">
        </div>
      </div>

      <div class="row">
        <div class="col-md-12"> Hello World
        </div>                
      </div>

    </div>
    <div class="col-md-3" >
      <table id="storyTable" class="table table-condensed">
        <thead>
          <tr>
            <th class="text-justify">#</th>
            <th class="text-center">Dates</th>
          </tr>
        </thead>
        <tbody>
          <script id="storyTable-template" type="text/x-handlebars-template">
          \{{#each story}}
          <tr>
            <td class="count text-justify" id="storyCount"></td>
            <td class="text-center">\{{ value }}</td>
          </tr>
          \{{/each}}
          </script>
        </tbody>
      </table>
      <!-- <ol id = "storyTable">
        <script id="storyTable-template" type="text/x-handlebars-template">
        \{{#each story}}
        <li>\{{ value }}</li>
        \{{/each}}
        </script>
      </ol> -->
    </div>
  </div>
</div>
<!-- jquery -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0-alpha1/jquery.min.js"></script>
<!-- bootstrap js -->
<script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
<!-- d3. needed for c3. -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.12/d3.min.js"></script>
<!-- c3. -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/c3/0.4.10/c3.min.js"></script>
<!-- Momentjs -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment.min.js"></script>
<!-- fooTable  -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-footable/3.0.4/js/footable.min.js"></script>
<!-- handlebars -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.3/handlebars.min.js"></script>
<!-- custom script -->
<script type="text/javascript" src="../scripts/show.js"></script>
<!-- custom chart helper script -->
<script type="text/javascript" src="../scripts/charts.js"></script>
</body>
</html>